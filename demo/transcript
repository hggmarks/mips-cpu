force -freeze sim:/cpu/reset St0 0
force -freeze sim:/cpu/reset St0 0
force -freeze sim:/cpu/reset St0 0
force -freeze sim:/cpu/reset St0 0
force -freeze sim:/cpu/reset St0 0
add wave -position 0  sim:/cpu/clk
wave create -driver freeze -pattern clock -initialvalue 1 -period 100ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/cpu/clk
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 10000ps sim:/cpu/reset
add wave -position 50 -format Logic -height 17 -editable 2 Edit:/cpu/reset
wave edit change_value -start 0ps -end 104ps -value 1 Edit:/cpu/reset
add wave -position end -format Logic -height 17 -editable 2 Edit:/cpu/reset
add wave -position 1 -format Logic -height 17 -editable 2 Edit:/cpu/clk
add wave -position 0 -format Logic -height 17 -editable 2 Edit:/cpu/clk
add wave -position 1 -format Logic -height 17 -editable 2 Edit:/cpu/reset
write format wave -window .main_pane.wave.interior.cs.body.pw.wf C:/Users/carva/Documents/GitHub/mips-cpu/demo/wave2.do
wave editwrite -file C:/Users/carva/Documents/GitHub/mips-cpu/demo/wave2.do -append
run
